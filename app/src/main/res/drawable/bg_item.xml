<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#FAF6D1" android:endColor="#C0A28C" android:angle="270"/>
            <corners android:radius="@dimen/_5sdp"/>
            <stroke android:width="@dimen/_2sdp" android:color="@color/brown" android:dashWidth="@dimen/_20sdp" android:dashGap="@dimen/_2sdp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#FFFEDB" android:endColor="#FFFDAE" android:angle="270"/>
            <corners
                android:topLeftRadius="@dimen/_5sdp"
                android:bottomLeftRadius="@dimen/_5sdp"
                android:topRightRadius="@dimen/_10sdp"
                android:bottomRightRadius="@dimen/_5sdp"/>
            <stroke android:width="@dimen/_1sdp" android:color="@color/brownEarth"/>
        </shape>
    </item>
</selector>